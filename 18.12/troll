print('Достигни 9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999 кликов и получи секретный материал')
for step in range(0,9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999):
    while (kb.is_pressed('ctrl')== True):
        count= count+1
        print(count)
    if (count >= 9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999):
