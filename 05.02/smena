g=0
mini=999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999
maxi=-999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999
b=0
indmaxi=0
indmini=0
a=[]
n=int(input())
while g != n:
    j = int(input())
    a.append(j)
    g = g +1
print(a)
for i in a:
    if i < mini:
        mini = i
    if i >maxi:
        maxi = i
for i in range(len(a)):
    if mini == a[i]:
        indmini=i
    if maxi == a[i]:
        indmaxi=i
    b=a[indmini]
    a[indmini]=a[indmaxi]
    a[indmaxi]=b
print(a)
