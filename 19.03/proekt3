    if f == "calculator":
        def vichit(r,t):
            x= r-t
            print(x)
        def slad(r,t):
            x= r+t
            print(x)
        def ymnoj(r,t):
            x= r*t
            print(x)
        def delok(r,t):
            x= r/t
            print(x)

        q=input('Что считать будем?: ')
