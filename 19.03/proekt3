def IIyatxIIyat(N):
    
        pp=0
        a=[[]]
        b=[]
        spisok=[[]]
        nit=0
        h=0
        while N!=h:
            b.append("▇")
            h=h+1
        h=0
        while N != h:
            a.append(b)
            spisok.append(b)
            h=h+1
        for i in range(len(a)):
            for j in range(len(a[i])):
                print(a[i][j], end = ' ')
            print(' ')
        



