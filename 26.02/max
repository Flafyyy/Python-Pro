a=-9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999
c=0
v=0
b=0
def maximum(c,v,b):
    global a
    c=int(input())
    v=int(input())
    b=int(input())
    if c > a:
        a=c
    if v>a:
        a=v
    if b > a:
        a=b
    print(a)
maximum(c,v,b)
