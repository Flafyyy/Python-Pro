a=[]
#КОМАНДЫЫЫЫЫЫЫЫЫЫЫЫЫЫЫЫЫЫЫЫЫ
#ПЕРВАЯ: ДОБАВИТЬВА
#ВТОРАЯ: ТОЧКА_ДЕЛ_ПО_ИНДЕКСУ
#ТРЕТЬЯ: ТОЧКА_ДЕЛ_ПО_НАЗВАНИЮЮЮЮ
#ЧЕТВЁРТАЯЯЯЯ: ПОКАЗЭЛПОНОМЕРУ
#ПЯЯЯЯЯТТТТАААЯ: ПОКАЗАТЬВЕСЬСПИСОК
#ааааа: !выход
#∞: ЗАМЕНАЫЫ
i = 0
while i != '!выход':
    i = str(input('КОМАНДУ ВВОДиТЬ БУДЕШЬ? '))
    if i == 'ДОБАВИТЬВА':
        u=str(input('ШО ТЕБЕ ДОБАВИТЬ??? '))
        a.append(u)
    if i == 'ТОЧКА_ДЕЛ_ПО_ИНДЕКСУ':
        g = int(input('ШО ТЕ УДАЛИТЬ ТА ПО ИНДЕКСУ, А!?!??!, НО ЦЕЛОЕ ЧИСЛО!!! ПОНЯЛ?!?!?! '))
        a.pop(g)
    if i == 'ТОЧКА_ДЕЛ_ПО_НАЗВАНИЮЮЮЮ':
        h = input('НУ ШО ДАВАЙ ГОВОРИ ШО ТЕБЕ ТАМ ПО НАЗВАНИЮ УДАЛИТЬ НАДО ')
        a.remove(h)
    if i == 'ПОКАЗЭЛПОНОМЕРУ':
        j = int(input('БЫСТРОСКАЗАЛЧТОТЕБЕНАДОПОКАЗАТЬ!!'))
        print(a[j])
    if i == 'ПОКАЗАТЬВЕСЬСПИСОК':
        print(a)
    if i == 'ЗАМЕНАЫЫ':
        y=int(input('ИНДЕКСИБЫСТРО!!'))
        b=str(input('ШО ТЕБЕ ТА ПО ИНДЕКСУ ЗАМЕНИТЬ!!?!?!?!??!'))
        a[y] = b
    if i == '!выход':
        print('НУ ШО ПОКА!')
